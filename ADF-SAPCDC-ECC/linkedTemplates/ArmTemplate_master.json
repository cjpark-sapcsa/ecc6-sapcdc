{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-SAPCDC-ECC"
		},
		"SAPCDC2ECC6_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SAPCDC2ECC6'"
		},
		"adls4sapecc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls4sapecc'"
		},
		"SAPCDC2ECC6_properties_typeProperties_clientId": {
			"type": "string",
			"defaultValue": "100"
		},
		"SAPCDC2ECC6_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "10steps2s4"
		},
		"SAPCDC2ECC6_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "20.16.15.220"
		},
		"SAPCDC2ECC6_properties_typeProperties_systemNumber": {
			"type": "string",
			"defaultValue": "00"
		},
		"adls4sapecc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sapeccqas2adf.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-SAPCDC-ECC_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls4sapecc_accountKey": {
						"value": "[parameters('adls4sapecc_accountKey')]"
					},
					"SAPCDC2ECC6_password": {
						"value": "[parameters('SAPCDC2ECC6_password')]"
					},
					"adls4sapecc_properties_typeProperties_url": {
						"value": "[parameters('adls4sapecc_properties_typeProperties_url')]"
					},
					"SAPCDC2ECC6_properties_typeProperties_clientId": {
						"value": "[parameters('SAPCDC2ECC6_properties_typeProperties_clientId')]"
					},
					"SAPCDC2ECC6_properties_typeProperties_userName": {
						"value": "[parameters('SAPCDC2ECC6_properties_typeProperties_userName')]"
					},
					"SAPCDC2ECC6_properties_typeProperties_server": {
						"value": "[parameters('SAPCDC2ECC6_properties_typeProperties_server')]"
					},
					"SAPCDC2ECC6_properties_typeProperties_systemNumber": {
						"value": "[parameters('SAPCDC2ECC6_properties_typeProperties_systemNumber')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}